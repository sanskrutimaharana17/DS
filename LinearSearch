#include<iostream>
using namespace std;

int main() {
	
	int num[20];
	int  n; //elements in array
	int key; //key = number to find
    
    cout<<"Enter number of elements";
    cin>>n;
    cout<<"enter key";
    cin>>key;
    
    cout<<"Enter elements" << endl;
    for(int i=0;i<n;i++){
    	cin>>num[i];
	}
	
	for(int i=0;i<n;i++){
		if(key==num[i]){
		    cout<<"number found"<<endl;	
		}
		
		
	}
	
	}
